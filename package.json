{
    "name": "rel",
    "icon": "rel.png",
    "displayName": "Rel",
    "description": "Rel Syntax Highlighting for VSCode",
    "version": "1.0.7",
    "author": {
        "name": "Mohammad Dashti and Amy Tabor",
        "email": "support@relational.ai"
    },
    "engines": {
        "vscode": "^1.51.0"
    },
    "keywords": [
        "visual studio code",
        "extension",
        "rel language",
        "syntax highlighter",
        "rel syntax highlighter"
    ],
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
		"onLanguage:rel"
	],
    "main": "extension.js",
    "contributes": {
        "languages": [{
            "id": "rel",
            "aliases": ["Rel", "rel"],
            "extensions": [".rel", ".delve"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "rel",
            "scopeName": "source.rel",
            "path": "./syntaxes/rel.tmLanguage.json"
        }]
    },
    "publisher": "RelationalAI",
    "repository": "https://github.com/RelationalAI-oss/Rel_VSCode"
}
